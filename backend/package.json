{
  "name": "playlist-converter-backend",
  "version": "1.0.0",
  "description": "Backend API for Spotify to YouTube Music playlist converter",
  "main": "dist/index.js",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "playlist",
    "converter",
    "spotify",
    "youtube-music",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^5.0.0",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.0.1",
    "express-rate-limit": "^7.5.1",
    "firebase": "^11.9.1",
    "firebase-admin": "^12.7.0",
    "google-auth-library": "^9.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/node": "^20.9.0",
    "rimraf": "^6.0.1"
  }
}
